<div class="container">
  <div class="row">
    <div class="col-md-12">
    <h1>  <%= User.find(session[:user_id]).name %> </h1>

    <table class="table">
      <thead>
        <tr>
          <th>Name</th>
          <th>E-mail</th>
          <th>Role</th>

          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% #@users.each do |user| %>
          <tr>
            <td>
            <% if !session[:user_id].nil? %>
              <%= User.find(session[:user_id]).name %>
            <% end %>
            </td>


          </tr>
        <% #end %>
      </tbody>
    </table>

    <br>

    <%= link_to 'New User', new_user_path %>

    </div>
  </div>
  <br><br>
</div>